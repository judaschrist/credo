{
	"name": "Make me happy Button",
	"version": "0.0.1",
	"description": "Download article in one click!",
	"permissions": ["activeTab", "declarativeContent","<all_urls>"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
    "page_action" : {	    
	    "default_title" : "Yes, happy"
    },
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	},
	"icons": {
		"16": "images/get_started16.png",
		"32": "images/get_started32.png",
		"48": "images/get_started48.png",
		"128": "images/get_started128.png"
	},
	"manifest_version": 2
}
